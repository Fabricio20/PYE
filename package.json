{
  "name": "pye",
  "version": "1.0.0",
  "description": "0.0.1",
  "main": "gulpfile.js",
  "scripts": {
    "build-html": "jade --out static/ src/views/",
    "build-js": "lsc -co static/s/js/ src/s/ls/",
    "build-css": "sass src/s/scss/:static/s/css/",
    "build-assets": "cp -r src/s/userscript/ static/s/userscript/ && cp -r src/s/img/ static/s/img/",
    "build": "npm run build-html && npm run build-js && npm run build-css && npm run build-assets"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seapunk/pye.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/seapunk/pye/issues"
  },
  "homepage": "https://github.com/seapunk/pye#readme",
  "devDependencies": {
    "jade": "^1.11.0",
    "livescript": "^1.6.0",
    "sass": "^1.43.4"
  }
}
